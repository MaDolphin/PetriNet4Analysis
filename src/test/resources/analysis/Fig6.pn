petrinet Fig6 {

    place p1 initial 1;
    place p2 initial 1;
    place p3;
    place p4;
    place p5;

    transition t1:
        1 <- p5
        1 -> p1
        1 -> p2

    transition t2:
        1 <- p1
        1 -> p3

    transition t3:
        1 <- p2
