petrinet Fig17b { // no boundedness, no liveness, reversibility
    place p1;
    place p2 initial 1;
    place p3;
    place p4 initial 1;
    place p5;

    transition t1:
        1 <- p5
        1 <- p2
        1 -> p4

    transition t2:
        1 <- p5
        1 -> p2

    transition t3:
        1 <- p3
        1 -> p4

    transition t4:
        1 <- p4
        1 -> p3
        1 -> p1

    transition t5:
        1 <- p1
        1 <- p2
        1 -> p5

}