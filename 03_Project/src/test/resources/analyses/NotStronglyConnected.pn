petrinet nichtStarkZusammenhaengend {
    // https://d-nb.info/968575072/34

    place p1 initial 1;
    place p2;
    place p3;
    place p4;
    place p5;
    place p6;
    place p7;
    place p8;

    transition t1:
    1 <- p1
    1 -> p2

    transition t2:
    1 <- p2
    1 -> p3

    transition t3:
    1 <- p3
    1 -> p4

    transition t4:
    1 <- p4
    1 -> p1

    transition t5:
    1 <- p4
    1 -> p5

    transition t6:
    1 <- p5
    1 -> p6

    transition t7:
    1 <- p6
    1 -> p7

    transition t8:
    1 <- p7
    1 -> p8

    transition t9:
    1 <- p8
    1 -> p5


}